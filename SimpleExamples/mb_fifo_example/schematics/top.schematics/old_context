{add_element driver -kind component -pointer 1 -associated_unit models:Driver -class Driver_pvt -include ../models/Driver_model.h} {add_element process -kind component -pointer 1 -associated_unit models:Process -class Process_pvt -include ../models/Process_model.h} {add_element sink -kind component -pointer 1 -associated_unit models:Sink -class Sink_pvt -include ../models/Sink_model.h} {add_element powerCtl -kind component -pointer 1 -associated_unit models:PowerCtl -class PowerCtl_pvt -include ../models/PowerCtl_model.h} { add_bind driver->master process->slave } { add_bind process->master sink->slave }
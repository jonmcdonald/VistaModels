{add_element axi_master -kind component -pointer 1 -associated_unit models:AXI_MASTER -class AXI_MASTER_pvt -include ../models/AXI_MASTER_model.h} {add_element axi_slave -kind component -pointer 1 -associated_unit models:AXI_SLAVE -class AXI_SLAVE_pvt -include ../models/AXI_SLAVE_model.h} {add_element apb_master -kind component -pointer 1 -associated_unit models:APB_MASTER -class APB_MASTER_pvt -include ../models/APB_MASTER_model.h} {add_element apb_slave -kind component -pointer 1 -associated_unit models:APB_SLAVE -class APB_SLAVE_pvt -include ../models/APB_SLAVE_model.h} { add_bind axi_master->master axi_slave->slave } { add_bind apb_master->master apb_slave->slave } { rename am axi_master } { rename as axi_slave }
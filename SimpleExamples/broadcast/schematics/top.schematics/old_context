{add_element cpu -kind component -pointer 1 -associated_unit models:AbstractCPU -class AbstractCPU_pvt -include ../models/AbstractCPU_model.h} {add_element bus -kind component -pointer 1 -associated_unit models:BroadcastBUS -class BroadcastBUS_pvt -include ../models/BroadcastBUS_model.h} {add_element d1 -kind component -pointer 1 -associated_unit models:Device -class Device_pvt -include ../models/Device_model.h} {add_element d2 -kind component -pointer 1 -associated_unit models:Device -class Device_pvt -include ../models/Device_model.h} {add_element d3 -kind component -pointer 1 -associated_unit models:Device -class Device_pvt -include ../models/Device_model.h} { add_bind cpu->cpu_master bus->cpu_slave } { add_bind bus->device1_master d1->slave } { add_bind bus->device2_master d2->slave } { add_bind bus->device3_master d3->slave }
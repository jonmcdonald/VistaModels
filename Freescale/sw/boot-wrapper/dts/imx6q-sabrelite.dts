				serial@02020000 {
					compatible = "fsl,imx6q-uart", "fsl,imx21-uart";
					reg = <0x2020000 0x4000>;
					interrupts = <0x0 0x1a 0x4>;
					clocks = <0x2 0xa0 0x2 0xa1>;
					clock-names = "ipg", "per";
					dmas = <0x4 0x19 0x4 0x0 0x4 0x1a 0x4 0x0>;
					dma-names = "rx", "tx";
					status = "okay";
					pinctrl-names = "default";
					pinctrl-0 = <0x7>;
				};

				esai@02024000 {
					reg = <0x2024000 0x4000>;
					interrupts = <0x0 0x33 0x4>;
				};

				ssi@02028000 {
					compatible = "fsl,imx6q-ssi", "fsl,imx51-ssi";
					reg = <0x2028000 0x4000>;
					interrupts = <0x0 0x2e 0x4>;
					clocks = <0x2 0xb2>;
					dmas = <0x4 0x25 0x1 0x0 0x4 0x26 0x1 0x0>;
					dma-names = "rx", "tx";
					fsl,fifo-depth = <0xf>;
					status = "okay";
					linux,phandle = <0x49>;
					phandle = <0x49>;
				};

				ssi@0202c000 {
					compatible = "fsl,imx6q-ssi", "fsl,imx51-ssi";
					reg = <0x202c000 0x4000>;
					interrupts = <0x0 0x2f 0x4>;
					clocks = <0x2 0xb3>;
					dmas = <0x4 0x29 0x1 0x0 0x4 0x2a 0x1 0x0>;
					dma-names = "rx", "tx";
					fsl,fifo-depth = <0xf>;
					status = "disabled";
				};

				ssi@02030000 {
					compatible = "fsl,imx6q-ssi", "fsl,imx51-ssi";
					reg = <0x2030000 0x4000>;
					interrupts = <0x0 0x30 0x4>;
					clocks = <0x2 0xb4>;
					dmas = <0x4 0x2d 0x1 0x0 0x4 0x2e 0x1 0x0>;
					dma-names = "rx", "tx";
					fsl,fifo-depth = <0xf>;
					status = "disabled";
				};

				asrc@02034000 {
					reg = <0x2034000 0x4000>;
					interrupts = <0x0 0x32 0x4>;
				};

				spba@0203c000 {
					reg = <0x203c000 0x4000>;
				};

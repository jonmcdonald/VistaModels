			arm-supply = <0x40>;
			pu-supply = <0x41>;
			soc-supply = <0x42>;
		};

		cpu@1 {
			compatible = "arm,cortex-a9";
			device_type = "cpu";
			reg = <0x1>;
			next-level-cache = <0x3f>;

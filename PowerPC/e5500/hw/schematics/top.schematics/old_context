{add_element cpu -kind component -pointer 1 -associated_unit models:e5500 -class e5500_pvt -include ../models/e5500_model.h} {add_element ic -kind component -pointer 1 -associated_unit models:IC -class IC_pvt -include ../models/IC_model.h} {add_element uart -kind component -pointer 1 -associated_unit Models_Catalogue:UART_16550 -class UART_16550_pvt -include models_catalogue.h} {add_element bus -kind component -pointer 1 -associated_unit models:BUS -class BUS_pvt -include ../models/BUS_model.h} {add_element mem -kind component -pointer 1 -associated_unit models:MEMORY -class MEMORY_pvt -include ../models/MEMORY_model.h} {add_element console -kind component -pointer 1 -associated_unit Models_Catalogue:CONSOLE -class CONSOLE_pvt -include models_catalogue.h} {add_element fabric -kind component -pointer 1 -associated_unit models:FPGA -class FPGA_pvt -include ../models/FPGA_model.h} {add_element fault -kind component -pointer 1 -associated_unit models:FAULT -class FAULT_pvt -include ../models/FAULT_model.h} { add_bind cpu->data_rw_initiator bus->data_rw } { add_bind cpu->insn_initiator bus->insn } { add_bind bus->ram mem->slave } { add_bind console->TX uart->receive } { add_bind uart->transmit console->RX } { add_bind bus->fpga fabric->slave } { add_bind fabric->irq ic->Internal1 } { add_bind fabric->master bus->from_fpga } { add_bind cpu->intc_if ic->IACK_read } { add_bind ic->reset_n cpu->reset_n } { add_bind ic->int_n cpu->int_n } { add_bind ic->cint_n cpu->cint_n } { add_bind ic->mcp_n cpu->mcp_n } { add_bind bus->ic ic->bus_slave } { add_bind uart->irq ic->Internal0 } { add_bind bus->uart uart->slave }